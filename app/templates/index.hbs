<section class="entry-create-container no-print">
  <CreateEntry
    @entry={{this.newEntry}}
    @isSaveErrored={{this.newEntryStateManager.isSaveErrored}}
    @startTimer={{this.startTimer}}
    @stopTimer={{this.stopTimer}}
    @searchProjects={{this.searchProjects}}
    @didUpdateEntry={{this.didUpdateNewEntry}}
    @retrySaveEntry={{this.retrySaveNewEntry}}
    @setProject={{this.setProjectOnNewEntry}} />
</section>
<EntriesByDay
  @list={{this.entryList}}
  @willUpdateEntry={{this.willUpdateEntry}}
  @didUpdateEntry={{this.didUpdateEntry}}
  @willDeleteEntry={{this.willDeleteEntry}}
  @didDeleteEntry={{this.didDeleteEntry}}
  @didRevertEntry={{this.didRevertEntry}}
  @restartEntry={{this.restartEntry}}
  @searchProjects={{this.searchProjects}} />
